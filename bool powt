#include <iostream>

using namespace std;

bool znajdzRowne(int t[], int rozmiar)
{
    for(int i=0; i<rozmiar; i++)
    {
        for(int j=(i+1); j<rozmiar; j++)
        {
            if(t[i]==t[j])
                return true;
        }
    }
    return false;
}

int main()
{
    int tablica[]={1, 2, 3, 4, 8};
    cout<<znajdzRowne(tablica, 5);
}
