#include <iostream>

using namespace std;

bool b(int t[], int r)
{
    for(int i=0; i<(r-1); i++)
    {
        for(int j=(r-1); j>i; j--)
        {
            if(t[i]==t[j] || t[j]<0 || t[i]<0)
                return false;
        }
    }
    return true;
}

int main()
{
    int tablica[] = {10, 2, 3, 4, 1, 6, 7};
    cout << b(tablica, 7);
}
