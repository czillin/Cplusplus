#include <iostream>

using namespace std;

int nwd(int a, int b)
{
    if(a>=b)
    for(int i=a; i>=1; i--)
    {
        if(a%i==0 && b%i==0){
            cout<<i<<" ";
            break;
        }
    }
    for(int i=b; i>=1; i--)
    {
        if(a%i==0 && b%i==0){
            cout<<i<<" ";
            break;
        }
    }
}


int main()
{
    nwd(14, 21);
}
