#include <iostream>

using namespace std;

void a(string s)
{
    int d = s.length();
    for(int i=0; i<d; i++)
    {
        if(s[i]!=' ')
        {
            cout<<s[i];
            for(int j=i+1; s[j]!=' '; j++)
            {
                s[j]=' ';
            }
        }
    }
}

int main()
{
    string w = " Cypr  Hiszpania    Urugwaj      Japonia ";
    a(w);
}
