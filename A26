#include <iostream>

using namespace std;

bool a(int t[], int r)
{
   for(int i=0; i<r; i++)
   {
       for(int j=(r-1); j>i; j--)
       {
           if(t[i]%t[j]==0 || t[j]%t[i]==0)
            return false;
       }
   }
   return true;
}

int main()
{
    int tablica[] = {3, 5, 7, 23};
    cout<<a(tablica, 4);
}
